<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.pawelsznuradev.f1pitstop.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_homeFragment_to_testItemFragment"
            app:destination="@id/testItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.pawelsznuradev.f1pitstop.ResultFragment"
        android:label="ResultFragment" />
    <fragment
        android:id="@+id/testItemFragment"
        android:name="com.pawelsznuradev.f1pitstop.ResultFragment"
        android:label="fragment_test_item_list"
        tools:layout="@layout/fragment_result_list" />
</navigation>